{
  "$schema": "../gen/schemas/desktop-schema.json",
  "identifier": "default",
  "description": "Capability for the main window",
  "windows": [
    "main"
  ],
  "permissions": [
    "core:default",
    "shell:allow-open",
    "store:default"
  ],
  "http": {
    "enabled": true,
    "allowedHosts": [
      "api.spotify.com",
      "accounts.spotify.com",
      "sdk.scdn.co"
    ]
  }
}
